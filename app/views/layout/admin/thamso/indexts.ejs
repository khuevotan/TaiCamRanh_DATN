
    <div class="page-content fade-in-up">
       

            <% if (typeof deleted !== 'undefined') { %>
                <%if (deleted == 'true') { %>

                    <div class="ibox ibox-success">
                        <div class="ibox-head">
                            <div class="ibox-title">   
                                <div style="color: rgb(255, 255, 255);">
                                    Xóa Thành Công!
                                </div>
                            </div>
                        </div>
                <% } %>
           
          <% } else { %>

            <div class="ibox ibox-info">
                <div class="ibox-head">
                    <div class="ibox-title">  
                        <div style="color: rgb(255, 255, 255);">
                            Danh Sách Các tham Số
                        </div>
                    
                    </div>
                </div>
            <% } %>
            <div class="ibox-body">
                
                <table class="table table-striped table-bordered table-hover" id="example-table" cellspacing="0" width="100%">
                    <thead>
                        <tr>
                            <th>MãThamSố</th>
                            <th>TênThamSố</th>
                            <th>GiáTrị</th>
                            <th>ChúThích</th>
                            <th>HànhĐộng</th>
                        </tr>
                    </thead>
                    <tbody>
                        <% thamso.forEach((item) => { %>
                        <tr>
                            <td><%= item.mats %></td>
                            <td><%= item.tents %></td>
                            <td><%= item.giatri %></td>
                            <td><%= item.chuthich %></td>
                          
                                <td>
                                    <a href="/admin/thamso/details/<%= item.mats %>" class="btn btn-default btn-xs m-r-5" data-toggle="tooltip" data-original-title="Details"><i class="fa fa-info font-14"></i></a>
                                    <a href="/admin/thamso/edit/<%= item.mats %>" class="btn btn-default btn-xs m-r-5" data-toggle="tooltip" data-original-title="Edit"><i class="fa fa-pencil font-14"></i></a>
                            
                                </td>
                        </tr>     
                        <% }) %>  
                    </tbody>
                </table>
            </div>
        </div>
    </div>

    <div style="height: 480px;"></div>

    
<script type="text/javascript">
    $(function() {
        $('#example-table').DataTable({
            pageLength: 10,
            dom: 'Bfrtip',
            scrollX: true,
            buttons: [
                'copy', 'csv', 'excel', 'pdf', 'print'
            ]
           
        });
    })
</script>



